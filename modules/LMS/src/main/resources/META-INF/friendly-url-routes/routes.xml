<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_2_0.dtd">

<routes>
    <route>
        <pattern></pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
    </route>
    <route>
        <pattern>/addEntry</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        <implicit-parameter name="mvcRenderCommandName">/lms/editEntry</implicit-parameter>
        <implicit-parameter name="email"></implicit-parameter>
    </route>
    <route>
        <pattern>/addEntry/{lmsId:\d+}/</pattern>
        <generated-parameter name="backurl">{backurl}</generated-parameter>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        <implicit-parameter name="mvcRenderCommandName">/lms/editEntry</implicit-parameter>
<!--    <implicit-parameter name="backurl"></implicit-parameter> -->
        
    </route>
</routes>